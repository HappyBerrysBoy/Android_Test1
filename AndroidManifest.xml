<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.unitloadsystem.activity"
    android:versionCode="2"
    android:versionName="1.1" >

    <uses-sdk
        android:minSdkVersion="11"
        android:targetSdkVersion="21" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppBaseTheme" >
        <activity
            android:name="com.unitloadsystem.activity.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.unitloadsystem.activity.UnitCalculationActivity"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.unitloadsystem.activity.KeyPadActivity"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.unitloadsystem.activity.CalculationResultActivity"
            android:label="@string/app_name" >
        </activity>
    </application>

</manifest>
